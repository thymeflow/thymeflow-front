<form class="card">
  <div class="card-header">
    <i class="fa fa-cog" aria-hidden="true"></i>
    <span>File upload</span>
  </div>
  <div class="card-block">
    <div class="form-group">
      {{file-upload uploadUrl=(concat apiEndpoint "/upload") uploadParam='file'}}
    </div>
    {{data-service-accounts model=model}}
  </div>
</form>
